{
	"$schema": "http://json-schema.org/draft-04/schema",
	"title": "Schema for a UniVote trustees",
	"description": "This JSON schema describes the format of the trustees definition in the UniVote system",
	"type": "object",
	"properties": {
		"mixerIds": {
			"type": "array",
			"description": "Identifiers of the mixers",
			"items": {
				"type":"string"
			}
		},
		"tallierIds": {
			"type": "array",
			"description": "Identifiers of the talliers",
			"items": {
				"type":"string"
			}
		}
	},
	"required": ["mixerIds", "tallierIds"]
}
