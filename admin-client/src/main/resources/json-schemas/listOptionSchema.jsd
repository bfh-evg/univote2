{
	"$schema": "http://json-schema.org/draft-04/schema",
	"title": "Schema of a list option",
	"type": "object",
	"properties": {
		"id":        { "type": "integer" },
		"type":      { "enum": ["listOption"] },
		"number":    { "type": "string" },
		"listName":  { "$ref": "i18nTextSchema.json" },
		"partyName": { "$ref": "i18nTextSchema.json" },
		"candidateIds": { "type": "array", "items": { "type": "integer" } }
	},
	"required": ["id", "type", "listName"],
	"additionalProperties": false
}
